<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="searchButton" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#onSearchClicked" prefHeight="25.0" prefWidth="89.0" text="Search Book" />
      <TextField fx:id="searchTxt" layoutX="107.0" layoutY="22.0" prefHeight="25.0" prefWidth="484.0" />
      <ListView fx:id="listViewBooks" onMouseClicked="#booklistViewClicked" layoutX="8.0" layoutY="59.0" prefHeight="248.0" prefWidth="585.0" />
      <Button fx:id="deleteBook" layoutX="275.0" layoutY="311.0" mnemonicParsing="false" onAction="#onDeleteBookClicked" text="Delete" />
   </children>
</Pane>
